[
  {
    "output": "file-resource.package.js",
    "include": [
      "File/ResourceGetter/Base",
      "File/ResourceAbstract",
      "File/LocalFileLink",
      "File/HttpFileLink",
      "File/LocalFile"
    ]
  },
  {
    "output": "file-error.package.js",
    "include": [
      "File/Error",
      "File/Error/*"
    ]
  },
  {
    "output": "file-utils.package.js",
    "include": [
      "File/utils/*"
    ]
  },
  {
    "output": "file-decorator.package.js",
    "include": [
      "File/Decorator/*"
    ]
  },
  {
    "output": "file-attach.package.js",
    "include": [
      "File/Attach",
      "File/Attach/Abstract",
      "File/Attach/Uploader",
      "File/Attach/Option/*",
      "File/Attach/Container/*"
    ]
  },
  {
    "output": "file-photocam.package.js",
    "include": [
      "File/ResourceGetter/PhotoCam",
      "File/ResourceGetter/PhotoCam/*"
    ],
    "exclude": [
      "File/ResourceGetter/PhotoCam/DialogPlugin"
    ]
  },
  {
    "output": "file-drop-area.package.js",
    "include": [
      "File/ResourceGetter/DropArea",
      "File/ResourceGetter/DropArea/*",
      "File/Directory"
    ]
  },
  {
    "output": "file-downloader.package.js",
    "include": [
      "File/Downloader",
      "File/Driver/*"
    ]
  }
]
