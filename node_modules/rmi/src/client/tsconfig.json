{
   "extends": "../../tsconfig.base.json",
   "compilerOptions": {
      "module": "amd",
      "rootDir": "./",
      "moduleResolution": "classic",
      "target": "es5",
      "lib": ["dom", "es5", "es2015"],
      "declaration": false,
      "noImplicitAny": false,
      "stripInternal": true,
      "sourceMap": false,
      "alwaysStrict": true,
      "traceResolution": false,
      "baseUrl": "./",
      "pretty": false,
      "diagnostics": false,
      "locale": "ru",
      "importHelpers": true,
      "experimentalDecorators": true
   },
   "include": [
      "types.d.ts",
      "SbisFile",
      "File",
      "Env"
   ]
}