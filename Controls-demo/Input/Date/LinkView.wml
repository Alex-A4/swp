               bind:startValue="_startValue"
               bind:endValue="_endValue"
               showPrevArrow="{{true}}"
               showNextArrow="{{true}}"
         />
         <Controls.Input.Date.LinkView
               startValue="{{_startValue}}"
               endValue="{{_endValue}}"
               style="linkMain"
         />
         <Controls.Input.Date.LinkView
               startValue="{{_startValue}}"
               endValue="{{_endValue}}"
               style="linkMain2"
         />
         <Controls.Input.Date.LinkView
               startValue="{{_startValue}}"
               endValue="{{_endValue}}"
               style="linkAdditional"
         />
      </div>
   </div>
</div>