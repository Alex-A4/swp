<div class="example-Number">
   <Controls-demo.Example.resource.BaseDemoInput title="Number input field" blocker="{{true}}">
      <ws:contents>
         <ws:Array>
            <ws:Object>
               <ws:template>
                  <div class="ws-flexbox ws-flex-column">
                     <div class="example-Base__specify">With tooltip inside</div>
                     <Controls.Input.Number placeholder="Count"
                                            precision="{{0}}"
                                            bind:value="_value1"/>
                  </div>
               </ws:template>
            </ws:Object>
            <ws:Object spacing="normal">
               <ws:template>
                  <div class="ws-flexbox ws-flex-column">
                     <div class="example-Base__specify">
                        <p>Input of integers with a set range</p>
                        <p>(0-999)</p>
                     </div>
                     <Controls.Input.Number placeholder="Count"
                                            precision="{{0}}"
                                            integersLength="{{3}}"
                                            onlyPositive="{{true}}"
                                            bind:value="_value2"/>
                  </div>
               </ws:template>
            </ws:Object>
            <ws:Object spacing="normal">
               <ws:template>
                  <div class="ws-flexbox ws-flex-column">
                     <div class="example-Base__specify">Input of integer and fractional part</div>
                     <Controls.Input.Number placeholder="Count"
                                            precision="{{2}}"
                                            textAlign="right"
                                            bind:value="_value3"/>
                  </div>
               </ws:template>
            </ws:Object>
         </ws:Array>
      </ws:contents>
   </Controls-demo.Example.resource.BaseDemoInput>
</div>