<div class="controls-BreadCrumbsView__crumb{{itemData.withOverflow ? ' controls-BreadCrumbsView__crumb_withOverflow'}}">
    <div class="controls-BreadCrumbsView__titleWrapper{{itemData.withOverflow ? ' controls-BreadCrumbsView__titleWrapper_withOverflow'}}"
         title="{{itemData.getPropValue(itemData.item, displayProperty || 'title')}}">
        <div class="controls-BreadCrumbsView__title">{{itemData.getPropValue(itemData.item, displayProperty || 'title')}}</div>
    </div>
    <ws:if data="{{ itemData.hasArrow }}">
        <span class="controls-BreadCrumbsView__arrow icon-size icon-DayForward icon-primary action-hover"></span>
    </ws:if>
</div>
