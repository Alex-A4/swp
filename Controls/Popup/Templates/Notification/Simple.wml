<ws:partial template="Controls/Popup/Templates/Notification/Base" iconClose="{{true}}" style="{{style}}" autoClose="{{autoClose}}">
    <ws:contentTemplate>
        <div class="controls-Notification__simpleTemplate">
            <ws:if data="{{icon}}">
                <div class="controls-Notification__simpleTemplate-icon icon-24 icon-{{style}} icon-{{icon}}"></div>
            </ws:if>
            <div class="controls-Notification__simpleTemplate-message">{{text}}</div>
        </div>
    </ws:contentTemplate>
</ws:partial>
